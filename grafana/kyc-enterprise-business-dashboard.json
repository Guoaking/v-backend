{
  "dashboard": {
    "id": null,
    "title": "KYC服务 - 企业级业务监控",
    "tags": ["kyc", "enterprise", "business", "otel"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "业务概览",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "业务成功率",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(business_operations_total{status=\"success\"}[5m]) / rate(business_operations_total[5m]) * 100",
            "legendFormat": "整体成功率",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 1}
      },
      {
        "id": 3,
        "title": "业务操作总数",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(business_operations_total)",
            "legendFormat": "总操作数",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 1}
      },
      {
        "id": 4,
        "title": "业务错误率",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(business_operations_total{status=\"failed\"}[5m]) / rate(business_operations_total[5m]) * 100",
            "legendFormat": "错误率",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 1}
      },
      {
        "id": 5,
        "title": "平均业务处理时间",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(business_operation_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 1}
      },
      {
        "id": 6,
        "title": "业务操作分布",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 5}
      },
      {
        "id": 7,
        "title": "各业务操作成功率",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(business_operations_total{status=\"success\"}[5m]) / rate(business_operations_total[5m]) * 100",
            "legendFormat": "{{operation}} 成功率",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6}
      },
      {
        "id": 8,
        "title": "业务操作速率",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(business_operations_total[5m])",
            "legendFormat": "{{operation}} - {{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6}
      },
